#MODEL='dc-cnn_multiple_2x_to_8x'
#MODEL='dc-cnn_multiple_mrbrain_t1_flair_only4x_checkdisplay'
#MODEL='dc-cnn_multiple_mrbrain_t1_flair_4_5_8x'
MODEL='dc-cnn_multiple_dataset_mask_acc_factor_458x'
BASE_PATH='/media/htic/NewVolume1/murali/MR_reconstruction'
#DATASET_TYPE='cardiac'
#DATASET_TYPE='mrbrain_t1','mrbrain_flair'
CHECKPOINT=${BASE_PATH}'/experiments/'${DATASET_TYPE}'/combined_dataset_mask_acc_factor_for_decouplelearning/'${MODEL}'/best_model.pt'
BATCH_SIZE=1
DEVICE='cuda:1'
USMASK_PATH=${BASE_PATH}'/Reconstruction-for-MRI/us_masks/'

for DATASET_TYPE in 'mrbrain_t1' 'mrbrain_flair'
    do
    for MASK_TYPE in 'cartesian' 'gaussian'
        do 
        for ACC_FACTOR in '4x' '5x' '8x'
        #for ACC_FACTOR in '4x'
            do 
            echo ${MASK_TYPE}','${ACC_FACTOR} 
            OUT_DIR=${BASE_PATH}'/experiments/combined_dataset_mask_acc_factor_for_decouplelearning/'${MODEL}'/results/'${DATASET_TYPE}'/'${MASK_TYPE}'/acc_'${ACC_FACTOR}
            DATA_PATH=${BASE_PATH}'/datasets/'${DATASET_TYPE}'/'${MASK_TYPE}'/validation/acc_'${ACC_FACTOR}
            echo python valid.py --checkpoint ${CHECKPOINT} --out-dir ${OUT_DIR} --batch-size ${BATCH_SIZE} --device ${DEVICE} --data-path ${DATA_PATH} --acceleration_factor ${ACC_FACTOR} --dataset_type ${DATASET_TYPE} --mask_type ${MASK_TYPE} 
            python valid.py --checkpoint ${CHECKPOINT} --out-dir ${OUT_DIR} --batch-size ${BATCH_SIZE} --device ${DEVICE} --data-path ${DATA_PATH} --acceleration_factor ${ACC_FACTOR} --dataset_type ${DATASET_TYPE} --mask_type ${MASK_TYPE} --usmask_path ${USMASK_PATH}
            done 
        done 
    done

